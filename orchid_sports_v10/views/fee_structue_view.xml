        <field name="res_model">od.fee.structure.pricelist</field>

        <field name="view_mode">list,form</field>
         <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            Click to add new PriceList. 
          </p>
        </field>
    </record>
 </data>
