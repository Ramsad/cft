<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- <data noupdate="1"> -->
    <data>




       <record id="seqeunce_od_sports_receipt" model="ir.sequence">
            <field name="name">Od Sports Receipt</field>
            <field name="code">od.sports.receipt</field>
            <field name="prefix">SR- %(month)s/%(day)s/</field>
            <field name="padding">3</field>
        </record>

         <record id="seqeunce_sports_receipt_line" model="ir.sequence">
            <field name="name">Sports Receipt Line</field>
            <field name="code">od.sports.receipt.line</field>
            <field name="prefix">INV</field>
            <field name="padding">3</field>
        </record>
        
    </data>
</odoo>

